<div style="padding:20px;">
	<h2>URL Management</h2>

	<table class="table table-condensed">
		<thead>
			<tr>
				<th>URI</th>
				<th>Title</th>
				<th>Handler</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
		{% for route in routes %}
				<tr>
					<td>{{route.uri}}</td>
					<td>{{route.title}}</td>
					<td>{{route.handlerName}}</td>
					<td><a class="btn btn-primary btn-mini">Edit</a><a class="btn btn-danger btn-mini" style="margin-left:10px;">Delete</a></td>
				</tr>
		{% endfor %}
		</tbody>
	</table>
</div>

<div class="modal">
	<div class="modal-header">
		<h3>Edit Route</h3>
	</div>
	<div class="modal-body">
	</div>
	<div class="modal-footer">
	</div>
</div>